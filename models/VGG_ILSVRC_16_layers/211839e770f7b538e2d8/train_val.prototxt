name: "VGG_ILSVRC_16_layer"
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 123
    mirror: true
    resize_short_side_min: 256
    resize_short_side_max: 512    
  }
  data_param {
    source: "/home/zyan3/local/data/imagenet/ilsvrc12/ilsvrc12_train_short_512_lmdb"
    batch_size: 100
    backend: LMDB
    selective_list: ""
  }
}
