name: "imagenet_nin4-small_new_version_train"
layer {
    name: "data"
    type: "Data"
    top: "data"
    top: "label"
    include {
        phase: TRAIN
    }    
    transform_param {
        crop_size: 224
        mean_value: 104
        mean_value: 117
        mean_value: 123
        mirror: true
        resize_short_side_min: 256
        resize_short_side_max: 512
    }    
    data_param {
        source: "/home/zyan3/local/data/imagenet/ilsvrc12/ilsvrc12_train_encoded_lmdb"
        batch_size: 128
        backend: LMDB
        selective_list: "/home/zyan3/proj/caffe_private_hdcnn/data/ilsvrc12/train.txt"
    }
}
layer {
    name: "data"
    type: "DataVariableSize"
    top: "data"
    top: "data_size"
    top: "label"
    include {
        phase: TEST
    }    
    transform_param {
        crop_size: 224
        mean_value: 104
        mean_value: 117
        mean_value: 123
        mirror: false
        resize_size: 256
    }    
    data_variable_size_param {
        source: "/home/zyan3/local/data/imagenet/ilsvrc12/ilsvrc12_val_encoded_lmdb"
        batch_size: 100
        backend: LMDB
        max_pixel_num: 864768
    }
}

layer {
    bottom: "data"
    top: "conv1"
    name: "conv1"
    type: "Convolution"
    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }        
    convolution_param {
        num_output: 96
        kernel_size: 11
        stride: 4
        weight_filler {
            type: "gaussian"
            mean: 0.
            std: 0.01
        }
        bias_filler {
            type: "constant"
            value: 0.
        }
     }
}
layer {
